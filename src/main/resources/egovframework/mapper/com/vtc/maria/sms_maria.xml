<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sms">
	<select id="munguBySitecode" parameterType="String" resultType="egovframework.veterans.com.cmm.service.vo.sms_mungu">
		select * from sms_mungu
		where 
			SiteCode = #{SiteCode}
	</select>
</mapper>